{
  "name": "pokenode-ts",
  "version": "1.16.1",
  "private": false,
  "description": "A lightweight Node.js wrapper for the PokéAPI with built-in types.",
  "keywords": [
    "pokeapi",
    "PokéAPI",
    "pokenode-ts",
    "Pokenode-ts",
    "pokemon",
    "pokedex"
  ],
  "homepage": "https://pokenode-ts-docs-gabb-c.vercel.app/",
  "bugs": {
    "url": "https://github.com/Gabb-c/pokenode-ts/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Gabb-c/pokenode-ts"
  },
  "license": "MIT",
  "author": {
    "name": "Gabb-c",
    "url": "https://github.com/Gabb-c"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup --config tsup.config.js",
    "lint": "eslint src --ext js,ts",
    "lint:fix": "eslint src --ext js,ts --fix",
    "prepare": "is-ci || husky install",
    "prepublishOnly": "pnpm build",
    "sort": "sort-package-json package.json",
    "test:ci": "vitest --run",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui --api 9527",
    "test:dev": "vitest -w --logHeapUsage --inspect"
  },
  "dependencies": {
    "axios": "^1.2.4",
    "axios-cache-interceptor": "^1.0.0",
    "pino": "^8.8.0",
    "pino-pretty": "^9.1.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^9.0.2",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.7",
    "@semantic-release/npm": "^9.0.2",
    "@semantic-release/release-notes-generator": "^10.0.3",
    "@swc/core": "^1.3.28",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitest/ui": "^0.28.1",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "http-status-codes": "^2.2.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.0",
    "lockfile-lint": "^4.10.0",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "semantic-release": "^20.0.4",
    "sort-package-json": "^2.2.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.4",
    "vitest": "^0.28.1"
  },
  "engines": {
    "node": ">=14"
  }
}
